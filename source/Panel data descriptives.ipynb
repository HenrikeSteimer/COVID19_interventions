   "source": [
    "# Rename variables for better distinction\n",
    "df = df.rename(columns={\"SD\": \"SDA\", \"SD_2\": \"SDA_2\", \"SD_3\": \"SDA_3\"})"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 183,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Make date variable a date\n",
    "df[\"date\"] = pd.to_datetime(df[\"date\"])"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Reduce information to unique intervention types"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 184,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Which unique kinds of interventions do we have?\n",
    "npis_kind = [\"CPV\", \"GS\", \"LD\", \"NESC\", \"PC\", \"SDA\", \"SDO\"]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 185,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Function to create dummy variable indicating whether any intervention of certain kind is in place\n",
    "\n",
    "# INPUTS: df - dataframe, npis_kind - list of unique kinds of interventions\n",
    "# OUTPUT: df with additional/overwritten dummy\n",
    "def reduce_to_one_per_kind(df, npis_kind):\n",
    "    for npi in npis_kind:\n",
    "        print(npi) # to see which kinds go through\n",
    "        npi_list = df.filter(regex=npi) # capture of all variables that capture information on this npi\n",
    "        npi_list = list(npi_list.columns) # make it a list\n",
    "        df[npi] = df[npi_list].max(axis=1) # overwrite/create dummy that is one if any npi of this kind is in place\n",
    "    return df"
   ]
  },
